{
  "name": "fetchomatic",
  "version": "0.0.1",
  "description": "Wrap fetch with retries, timeout, logging, caching, error handling and more",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm-wrapper.mjs"
    },
    "./esm": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "fetch",
    "http",
    "promise",
    "request",
    "curl",
    "wget",
    "xhr",
    "whatwg",
    "https",
		"http2",
		"get",
		"got",
		"url",
		"uri",
		"request",
		"simple",
		"curl",
		"wget",
		"fetch",
		"net",
		"network",
		"gzip",
		"brotli",
		"requests",
		"human-friendly",
		"axios",
		"superagent",
		"node-fetch",
		"ky"
  ],
  "author": "fetch",
  "license": "ISC",
  "bin": "dist/bin.js",
  "files": [
    "dist",
    "*.md"
  ],
  "scripts": {
    "test": "playwright test",
    "test-all": "pnpm build && concurrently 'npm:test:*'",
    "test:node": "playwright test",
    "test:deno": "DENO_NO_PACKAGE_JSON=true deno test test/deno --no-check --allow-net --allow-read --trace-ops",
    "test:bun": "bun test test/bun",
    "test:pkg": "(cd test/pkg && pnpm setup) && TEST_DIR=test/pkg pnpm test",
    "test-server": "ts-node test/server.ts",
    "playwright": "playwright",
    "playwright:browsers": "playwright install --with-deps",
    "eslint": "eslint --ext '.ts,.js,.md'",
    "lint": "tsc && eslint .",
    "build": "rm -rf dist && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && node scripts/wrap.cjs"
  },
  "devDependencies": {
    "@playwright/test": "^1.36.1",
    "@types/eslint": "8.37.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.3",
    "concurrently": "^8.2.0",
    "eslint": "8.40.0",
    "eslint-plugin-mmkal": "0.1.2",
    "expect-type": "^0.15.0",
    "express": "^4.18.2",
    "isomorphic-fetch": "^3.0.0",
    "keyv": "^4.5.3",
    "make-fetch-happen": "^11.1.1",
    "minipass-fetch": "^3.0.3",
    "next": "^13.4.1",
    "node-fetch": "^2.6.2",
    "np": "7.7.0",
    "strip-indent": "^4.0.0",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4",
    "xstate": "^4.37.2",
    "zod": "^3.21.4"
  },
  "np": {
      "cleanup": false
  }
}
